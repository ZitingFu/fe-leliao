/* page/pages/login/login.wxss */
.container {
    position:fixed;
    width:100%;
    height:100%;
    background:#fff;
    display: block;
}
.container .title {
    font-size:42rpx;
    text-align:center;
    color:#444444;
    margin:80rpx auto 0rpx auto;
}
.container image{
    width: 365rpx;
    height:365rpx;
    margin:50rpx auto 0rpx auto;
    display:block;
    border-radius: 50rpx;
}
.container .login {
    width:610rpx;
    color: #fff;
    height: 100rpx;
    margin: 150rpx auto;
    text-align: center;
    line-height: 92rpx;
    background: #1aad19;
    border-radius: 10rpx;
    display:flex;
    justify-content:center;
    align-items:center;
}