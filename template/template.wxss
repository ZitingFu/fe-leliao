/* template/template.wxss */
.icon{
  width:54rpx;
  height: 54rpx;
}
.tabBar{
  width:100%;
  height: 116rpx;
  line-height: 116rpx;
  position: fixed;
  bottom:0;
  background:#fff;
  font-size:32rpx;
  color:#333;
  box-shadow: 6rpx 6rpx 6rpx 6rpx #aaa;
  display: flex;
  z-index:5;
}

 .tabBar-item{
  text-align: center;
  overflow: hidden;
  flex: 1;
  display:flex;
}
.tabBar-photo {
    width: 160rpx;
    height: 100%;
    text-align: center;
    /* position: absolute;
    left: calc(50% - 80rpx); */
}
.tabBar-photo navigator {
    height: 100%;
    position: relative;
}
.tabBar-photo view {
    height: 100%;
    position: relative;
}
.tabBar-photo image {
    width: 160rpx;
    height: 160rpx;
    position: absolute;
    left:0;
    bottom: 30rpx;
}
/*当前字体颜色*/
.tabBartext{
  color:red;
}
.dian{
  position: absolute;
  top:25rpx;
  width: 18rpx;
  height: 18rpx;
  right: 47rpx;
}
.fun{
    width:100rpx;
    height:100rpx;
    margin:8rpx 0;
}
.home{
    width:50%;
    margin-left: 30%;
}
